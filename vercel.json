{
    "framework": "vite",
    "buildCommand": "npm install && npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/|_next/|static/|assets/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}