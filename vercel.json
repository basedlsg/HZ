{
    "buildCommand": "npm install && npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}